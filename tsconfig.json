{
  "compilerOptions": {
      "downlevelIteration": true,
      "maxNodeModuleJsDepth": 1,
      "target": "ES5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "baseUrl": "src/",
      "outDir": "dist/",
      "skipLibCheck": true,
      "lib": [
          "esnext"
      ]
  },
  "include": [
      "./**/*.ts"
  ]
}